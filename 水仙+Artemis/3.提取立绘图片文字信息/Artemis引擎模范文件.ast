        {"text"},
        text = {
            vo = {{"vo", ch="siyinna", file="Z000600557"},},
            ja = {{"「要是知道对方在担心的话，不会挤不出那么一点时间吧。如果是心爱的孩子就更是如此了。没有联系也就是说――」",},},
        },
        linkback = "block_00408",
        linknext = "block_00410",
        line = 914
    },

