        {"bg",id=5,lv=5,file="CGM_SI020101_010a_0000",time=300, path=":fg/",sync=0,x=420,y=0},
        {"text"},
        text = {
            pagebreak = true,
            vo = {{"vo", ch="siyinna", file="Z000600539"},},
            ja = {{"「你这样只是自我满足而已。只要流眼泪，就会有人温柔对你，才一直流泪的吧。这样做，只是在逃避现实。」",},},
        },
        linkback = "block_00394",
        linknext = "block_00396",
        line = 886
    },

    block_00396 = {
        {"text"},
        text = {
            vo = {{"vo", ch="li", file="Z5074"},},
            ja = {{"「………不要……再说了……」",},},
        },
        linkback = "block_00395",
        linknext = "block_00397",
        line = 888
    },

    block_00397 = {
        {"bg",id=5,lv=5,file="CGM_SI020114_010a_0000",time=300, path=":fg/",sync=0,x=420,y=0},
        {"text"},
        text = {
            vo = {{"vo", ch="siyinna", file="Z000600541"},},
            ja = {{"「差不多也该察觉到了吧。你的爸爸妈妈回不来了。再也不会。小提比再也不会见到爸爸和妈妈了。」",},},
        },
        linkback = "block_00396",
        linknext = "block_00398",
        line = 890
    },

    block_00398 = {
        {"bg",id=5,lv=5,file="CGM_SI020102_010a_0000",time=300, path=":fg/",sync=0,x=420,y=0},
        {"text"},
        text = {
            pagebreak = true,
            vo = {{"vo", ch="siyinna", file="Z000600543a"},},
            ja = {{"「有呢，我就直说了。黎也只是同情小提比才会在这里的。并没有像爸爸妈妈那样对小提比倾注爱情。黎仅仅是被哀怜束缚在这里的。小提比，你懂吧？你会一直一个人。」",},},
        },
        linkback = "block_00397",
        linknext = "block_00399",
        line = 892
    },

